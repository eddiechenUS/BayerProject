                    <ServiceImplementation
                     description=""
                     handlerName="Script"
                     name="ProcessData">
                        <ConfigurationTables>
                            <ConfigurationTable
                             description=""
                             isMultiRow="false"
                             name="Script"
                             ordinal="0">
                                <DataShape>
                                    <FieldDefinitions>
                                        <FieldDefinition
                                         baseType="STRING"
                                         description="code"
                                         name="code"
                                         ordinal="0"></FieldDefinition>
                                    </FieldDefinitions>
                                </DataShape>
                                <Rows>
                                    <Row>
                                        <code>
                                        <![CDATA[
                                        // result: INFOTABLE dataShape: "BayerProductionPlansParserSettingsDataShape"
                                        var parserSettings =  me.GetParserSettings();
                                        
                                        // receive data pushed by the other instance of TWX and process it
                                        logger.info(me.name + ".ProcessData received " + data.getRowCount() + " rows");
                                        logger.warn(data);
                                        var aggregatedData = {};
                                        
                                        var tableLength = data.rows.length;
                                        for (var x=0; x < tableLength; x++) {
                                            var row = data.rows[x];
                                            
                                            if (!row.productionLine) {
                                                logger.warn(me.name + ".ProcessData - no productionLine specified");
                                                continue;
                                            }
                                            
                                            row.productionLine = me.ProcessLineIdentifier({ lineName: row.productionLine });
                                            
                                            var lineThing = "BayerProductionLine_" + row.productionLine;
                                            if (!Things[lineThing]) {
                                                logger.warn(me.name + ".ProcessData - line " + lineThing + " doesn't exist"); 
                                            }
                                            
                                            if (aggregatedData[lineThing] === undefined) {
                                            	aggregatedData[lineThing] = Resources["InfoTableFunctions"].Clone({ t1: data });
                                                aggregatedData[lineThing].RemoveAllRows();
                                            }
                                            aggregatedData[lineThing].AddRow(row);
                                        }
                                        
                                        // result: INFOTABLE dataShape: "RootEntityList"
                                        var lines =  ThingTemplates["BayerProductionLine"].GetImplementingThings();
                                        
                                        var tableLength = lines.rows.length;
                                        for (var x=0; x < tableLength; x++) {
                                            var line = lines.rows[x];
                                            var plan = null;
                                         
                                            try {    
                                                if (aggregatedData[line.name] === undefined) {
                                                    // no data for line was provided
                                                    if (parserSettings.useBlankItemIfNoDataForLine) {
                                                        // CreateInfoTableFromDataShape(infoTableName:STRING("InfoTable"), dataShapeName:STRING):INFOTABLE(BayerProductionPlansParserColumnMappingDataShape)
                                                        plan = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape({ dataShapeName : "BayerProductionPlansParserColumnMappingDataShape" });
                                                        // BayerProductionPlansParserColumnMappingDataShape entry object
                                                        var newEntry = new Object();
                                                        newEntry.productionLine = line.name; // STRING [Primary Key]
                                                        newEntry.productId = 'standby'; // STRING
                                                        plan.AddRow(newEntry);
                                                        logger.debug(me.name + ".ProcessData - standby added for line " + line.name);
                                                    }
                                                }
                                                else {
                                                    plan = aggregatedData[line.name];
                                                }
                                        
                                                if (plan) {
                                                    var result =  Things[line.name].LoadProductionPlan({
                                                        data: plan /* INFOTABLE */
                                                    });
                                                }
                                                else {
                                                    logger.info(me.name + ".ProcessData - no production plan provided for line " + line.name);
                                                }
                                            }
                                            catch (ex) {
                                                logger.error(me.name + ".ProcessData - error loading plan for " + line.name + " | " + ex);
                                            }
                                        }
                                        ]]>
                                        </code>
                                    </Row>
                                </Rows>
                            </ConfigurationTable>
                        </ConfigurationTables>
