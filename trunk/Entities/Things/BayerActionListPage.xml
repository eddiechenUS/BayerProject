                                    <Row>
                                        <code>
                                        <![CDATA[
                                        var params = {
                                        	infoTableName : "InfoTable",
                                        	dataShapeName : "BayerActionListDataShape"
                                        };
                                        
                                        // CreateInfoTableFromDataShape(infoTableName:STRING("InfoTable"), dataShapeName:STRING):INFOTABLE(BayerActionListDataShape)
                                        var actionList = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape(params);
                                        
                                        var textCsv;
                                        var params = {
                                        	createdFrom: undefined /* DATETIME */,
                                        	productionLine: undefined /* THINGNAME */,
                                        	dueDate: undefined /* DATETIME */,
                                        	createdTo: undefined /* DATETIME */,
                                        	assignedTo: assignedTo /* USERNAME */,
                                        	status: undefined /* STRING */
                                        };
                                        actionList = me.GetActionList(params);
                                        
                                        for (var i = 0; i <actionList.rows.length; i++) {
                                            textCSV += actionList[i].assignedTo;
                                            textCSV += ",";
                                            textCSV += actionList[i].status;
                                            textCSV += ",";
                                            textCSV += actionList[i].createdFrom;
                                            textCSV += ",";
                                            textCSV += actionList[i].createdTo;
                                            textCSV += ",";
                                            textCSV += actionList[i].dueDate;
                                            textCSV += ",";
                                            textCSV += actionList[i].productionLine;
                                            textCSV += "\n"; 
                                            
                                        }
                                        var params = {
                                        	path: "testFile.csv" /* STRING */,
                                        	data: textCSV /* STRING */,
                                        	overwrite: true /* BOOLEAN */
                                        };
                                        
                                        // no return
                                        Things["SystemRepository"].CreateTextFile(params);
                                        ]]>
                                        </code>
                                    </Row>
                                </Rows>
