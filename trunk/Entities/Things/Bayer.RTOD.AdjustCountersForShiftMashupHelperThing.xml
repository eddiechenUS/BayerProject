                             name="Script"
                             ordinal="0">
                                <DataShape>
                                    <FieldDefinitions>
                                        <FieldDefinition
                                         baseType="STRING"
                                         description="code"
                                         name="code"
                                         ordinal="0"></FieldDefinition>
                                    </FieldDefinitions>
                                </DataShape>
                                <Rows>
                                    <Row>
                                        <code>
                                        <![CDATA[
                                        //Create output infotable
                                        var params = {
                                            infoTableName : "InfoTable",
                                            dataShapeName : "AdjustCountersDS"
                                        };
                                        
                                        // CreateInfoTableFromDataShape(infoTableName:STRING("InfoTable"), dataShapeName:STRING):INFOTABLE(AdjustCountersDS)
                                        var outputinfotableRaw = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape(params);
                                        
                                        
                                        // result: INFOTABLE dataShape: "BayerShiftSchedulesDataShape"
                                        var resultset =  Things["BayerProductionLine_086"].GetShiftsFromNow({  //test
                                        	nrOfPrevShifts: undefined /* STRING */,
                                        	interval: 30 /* INTEGER */,
                                        	nrOfNextShifts: undefined /* STRING */
                                        });
                                        
                                        var scheduleTable = resultset.shiftSchedule;
                                        
                                        var tableLength = scheduleTable.rows.length;
                                        for (var x=0; x < tableLength; x++) {
                                            var row = scheduleTable.rows[x];
                                            var  tempEmptyRow= new Object();
                                            var randomNumber1 = Math.floor(Math.random()*1000);
                                            var randomNumber2 = Math.floor(Math.random()*1000);
                                            tempEmptyRow.Reject=randomNumber1;//test data
                                            tempEmptyRow.Good = randomNumber2;//test data
                                            //tempEmptyRow.Reject=row.rejectCounter;
                                            //tempEmptyRow.Good = row.actul___;
                                            tempEmptyRow.Hour=row.time;
                                            outputinfotableRaw.AddRow(tempEmptyRow);
                                        }
                                        
                                        me.SourceTable=outputinfotableRaw;
                                        me.TableToBeAdjust=outputinfotableRaw;
                                        
                                        result = outputinfotableRaw;
                                        ]]>
                                        </code>
                                    </Row>
                                </Rows>
                            </ConfigurationTable>
                        </ConfigurationTables>
