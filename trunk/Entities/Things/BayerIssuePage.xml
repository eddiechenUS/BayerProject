                                        <code>
                                        <![CDATA[
                                        var params = {
                                        	infoTableName : "InfoTable",
                                        	dataShapeName : "BayerHoursWithDatesDataShape"
                                        };
                                        
                                        // CreateInfoTableFromDataShape(infoTableName:STRING("InfoTable"), dataShapeName:STRING):INFOTABLE(NumberArrayElement)
                                        var result = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape(params);
                                        
                                        if (startHour != undefined && endHour != undefined) {
                                            if (startHour < endHour) {
                                                for (var i = startHour; i < endHour; i++) {
                                                    // NumberArrayElement entry object
                                                    var newEntry = new Object();
                                                    newEntry.hour = i; // NUMBER
                                                    newEntry.date = dateAddHours(startDate, i - startHour);
                                                    result.AddRow(newEntry);
                                                }
                                            }
                                            else {
                                                for (var i = startHour; i <= 23; i++) {
                                                    // NumberArrayElement entry object
                                                    var newEntry = new Object();
                                                    newEntry.element = i; // NUMBER
                                                    newEntry.date = dateAddHours(startDate, i - startHour);
                                                    result.AddRow(newEntry);
                                                }
                                                for (var i = 0; i < endHour; i++) {
                                                    // NumberArrayElement entry object
                                                    var newEntry = new Object();
                                                    newEntry.element = i; // NUMBER
                                                    newEntry.date = dateAddDays(dateAddHours(startDate, i - startHour), 1); // dateAddHours doesn't increase a day if past midnight; dateAddDays increases month/year if needed
                                                    result.AddRow(newEntry);
                                                }
                                            }
                                        }
                                        ]]>
                                        </code>
                                    </Row>
                                </Rows>
