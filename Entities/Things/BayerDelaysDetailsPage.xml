                <ServiceImplementations>
                    <ServiceImplementation
                     description=""
                     handlerName="Script"
                     name="GetMachineState">
                        <ConfigurationTables>
                            <ConfigurationTable
                             description=""
                             isMultiRow="false"
                             name="Script"
                             ordinal="0">
                                <DataShape>
                                    <FieldDefinitions>
                                        <FieldDefinition
                                         baseType="STRING"
                                         description="code"
                                         name="code"
                                         ordinal="0"></FieldDefinition>
                                    </FieldDefinitions>
                                </DataShape>
                                <Rows>
                                    <Row>
                                        <code>
                                        <![CDATA[
                                        var events = Things["BayerDBDataProvider"].GetShiftEvents({
                                        	shiftScheduleId: shiftId /* INTEGER */
                                        });
                                        
                                        var shift = 28800000; //ms = 8h
                                        var now = new Date();
                                        var shiftLeft = shift;
                                        
                                        // CreateInfoTableFromDataShape(infoTableName:STRING("InfoTable"), dataShapeName:STRING):INFOTABLE(DelaysDetailsMaschineStateDataShape)
                                        var result = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape({
                                            infoTableName : "InfoTable",
                                            dataShapeName : "DelaysDetailsMaschineStateDataShape"
                                        });
                                        
                                        var lastEventTime = new Date(shiftStartTime);
                                        var lastEventCategory = '';
                                        
                                        if (events.getRowCount() > 0) {
                                            var tableLength = events.rows.length;
                                            for (var x=0; x < tableLength; x++) {
                                                var event = events.rows[x];
                                                var time = event.startTime;
                                                var duration = event.durationMs;
                                                //if event starts in prev shift
                                                if (event.startTime.getTime() <= shiftStartTime.getTime()) {
                                                    time = shiftStartTime;
                                                    duration = dateDifference(event.endTime, shiftStartTime);
                                                } else {
                                                	if (x === 0 && event.category !== 'running') {
                                                        duration = dateDifference(event.startTime, shiftStartTime); 
                                                        var startShift = new Object();
                                                        startShift.duration = duration;
                                                        startShift.sectionColorState = 'running'; // STRING
                                                        startShift.time = shiftStartTime; // DATETIME
                                                        result.AddRow(startShift);
                                                        
                                                        shiftLeft-= duration;
                                                        duration = event.durationMs;
                                                    }
                                                }
                                               
                                                event.time = time;
                                                event.duration = duration;
                                                result.AddRow(event);
                                               	
                                                shiftLeft-= duration;
                                                lastEventTime = event.endTime;
                                                lastEventCategory = event.category;
                                            }
                                        }
                                        
                                        var isCurrentShift = now.getTime() < dateAddHours(shiftStartTime,8).getTime() && now.getTime() >= shiftStartTime.getTime();
                                        if (shiftLeft > 0 && isCurrentShift) {
                                            // result: INFOTABLE dataShape: "BayerProductionPlanDataShape"
                                            var currentProduct = Things[line].GetCurrentProduct();
                                        	var isChangeover =  currentProduct.productId == "Cambio" || !currentProduct.targetQuantity;
                                            
                                            var lastEventTillNow = new Object();
                                            lastEventTillNow.time = lastEventTime; // DATETIME
                                            lastEventTillNow.duration = dateDifference(now, lastEventTime);
                                            lastEventTillNow.sectionColorState = isChangeover ? "changeover" : Things[line].productionRunning ? 'running' : 'breakdown';
                                            result.AddRow(lastEventTillNow);
                                            
                                            shiftLeft-= lastEventTillNow.duration;
                                            lastEventTime = dateAddMilliseconds(lastEventTillNow.time, lastEventTillNow.duration);
                                            lastEventCategory = lastEventTillNow.sectionColorState;
                                        }
                                           	
                                        if (shiftLeft > 0) {
                                           var endShift = new Object();
                                           endShift.time = lastEventTime; // DATETIME
                                           endShift.sectionColorState = 'gap';
                                           endShift.duration = shiftLeft;
                                           result.AddRow(endShift);
                                        }
                                        ]]>
                                        </code>
                                    </Row>
                                </Rows>
                            </ConfigurationTable>
                        </ConfigurationTables>
                    </ServiceImplementation>
                    <ServiceImplementation
