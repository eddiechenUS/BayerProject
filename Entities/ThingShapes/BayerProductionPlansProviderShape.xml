                 name="AddProductionPlanItems">
                    <ConfigurationTables>
                        <ConfigurationTable
                         description=""
                         isMultiRow="false"
                         name="Script"
                         ordinal="0">
                            <DataShape>
                                <FieldDefinitions>
                                    <FieldDefinition
                                     baseType="STRING"
                                     description="code"
                                     name="code"
                                     ordinal="0"></FieldDefinition>
                                </FieldDefinitions>
                            </DataShape>
                            <Rows>
                                <Row>
                                    <code>
                                    <![CDATA[
                                    var tableLength = items.rows.length;
                                    for (var x=0; x < tableLength; x++) {
                                        var row = items.rows[x];
                                        
                                        var params = {};
                                        // infotable datashape iteration
                                        var dataShapeFields = items.dataShape.fields;
                                        for (var fieldName in dataShapeFields) {
                                            params[fieldName] = row[fieldName];
                                        }
                                    
                                        // result: NUMBER
                                        var result =  Things["BayerSqlServerConnector"].InsertProductionPlan(params);
                                    }
                                    ]]>
                                    </code>
                                </Row>
                            </Rows>
                        </ConfigurationTable>
                    </ConfigurationTables>
